<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mycom.hims.scheduler.SetUpActivity">

    <Button
        android:layout_width="80dp"
        android:layout_height="118dp"
        android:background="@drawable/off_xtouch"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="23dp"
        android:id="@+id/btn_lockOnOff"
        />
    
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:layout_marginLeft="129dp"
        android:textSize="60sp"
        android:gravity="center"
        android:id="@+id/tv_OnOff"
        />


    <TextView
        android:layout_marginTop="159dp"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/noButtonBackgroundColor"
        />
    
    <Button
        android:layout_width="85dp"
        android:layout_height="83dp"
        android:background="@drawable/logouticon_xtouch"
        android:layout_marginTop="206dp"
        android:layout_marginLeft="30dp"
        android:id="@+id/btn_logout"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="204dp"
        android:layout_marginLeft="129dp"
        android:textSize="60sp"
        android:gravity="center"
        android:textColor="@color/white"
        android:text="@string/logout"
        android:id="@+id/tv_logout"
        />

</RelativeLayout>
