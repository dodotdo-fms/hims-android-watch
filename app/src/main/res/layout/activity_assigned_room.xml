<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mycom.hims.scheduler.AssignedRoomActivity"
    android:background="#000">


    <com.example.mycom.hims.view.MyRecyclerView
        android:id="@+id/list_room"
        android:layout_marginTop="100dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical"
        />

    <!--<RelativeLayout--> -->

    <ImageView
        android:layout_width="160dp"
        android:layout_height="100dp"
        android:id="@+id/room_back"
        android:onClick="back_click"
        android:background="@drawable/back"
/>
    
    <TextView
        android:layout_alignParentRight="true"
        android:layout_marginRight="23dp"
        android:textSize="67sp"
        android:text=""
        android:id="@+id/tv_willCleanRoom"
        android:textColor="@color/red"
        android:layout_width="39dp"
        android:layout_height="78dp" />
    
 >

</RelativeLayout>
