<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mycom.hims.scheduler.PasswordActivity"
    android:background="#000">
    <include layout="@layout/view_loading"
        android:id="@+id/view_loading"
        />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/search_back"
        android:onClick="onClick"
        android:src="@drawable/search_back"
        android:paddingLeft="14dp"
        android:paddingRight="10dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/search_num"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textColor="#00c9ff"
        android:paddingTop="4px"
        android:paddingRight="12px"
        android:textSize = "15pt"/>

    <TextView
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:text="1"
        android:onClick="onClick"
        android:id="@+id/btn1"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        android:clickable="true"
        />

    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="70dp"
        android:text="2"
        android:id="@+id/btn2"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />

    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="70dp"
        android:text="3"
        android:id="@+id/btn3"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />
    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="135dp"
        android:text="4"
        android:id="@+id/btn4"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />
    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="135dp"
        android:text="5"
        android:id="@+id/btn5"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />

    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="135dp"
        android:text="6"
        android:id="@+id/btn6"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />

    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="195dp"
        android:text="7"
        android:id="@+id/btn7"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />
    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="195dp"
        android:text="8"
        android:id="@+id/btn8"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />

    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="195dp"
        android:text="9"
        android:id="@+id/btn9"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />
    <ImageView

        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="255dp"
        android:id="@+id/btn_cancel"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:src="@drawable/cancel"
        />
    <TextView
        android:clickable="true"
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="255dp"
        android:text="0"
        android:id="@+id/btn0"
        android:paddingLeft="35dp"
        android:textColor="#9a9ca6"
        android:textSize="13pt"
        />
    <ImageView
        android:onClick="onClick"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="255dp"
        android:src="@drawable/login_button"
        android:id="@+id/btn_search"
        android:paddingLeft="0dp"
        />
    <RelativeLayout
        android:layout_width="320dp"
        android:layout_height="320dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:background="#000"
        android:id="@+id/lay">

        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="60dp"
            android:background="#242424">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="warning!\nPlease check\nagain."
                android:textColor="#fff"
                android:gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:textSize = "9pt"/>

            <ImageView
                android:onClick="onClick"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="137dp"
                android:src="@drawable/login_ok"
                android:id="@+id/ok_btn"
                />
        </RelativeLayout>

    </RelativeLayout>

</RelativeLayout>
